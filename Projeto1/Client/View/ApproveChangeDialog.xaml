<Window x:Class="Client.View.ApproveChangeDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Quotation changed" Height="144.318" Width="356.773" ResizeMode="NoResize" WindowStartupLocation="CenterOwner" Icon="/Client;component/Resources/favicon.ico" ShowInTaskbar="False">
    <Grid>
        <TextBlock Margin="0,20,0,0" TextWrapping="Wrap" Text="The quotation changed! Do you want to keep your order?" Height="24" Width="314" VerticalAlignment="Top" HorizontalAlignment="Center" Foreground="#FF1085BB"/>
        <Button x:Name="Approve" Content="Yes" HorizontalAlignment="Left" Margin="166,62,0,0" VerticalAlignment="Top" Width="74" Click="Approve_Click" Foreground="#FF0B637E"/>
        <Button x:Name="Disapprove"  Content="No" HorizontalAlignment="Left" Margin="246,62,0,0" VerticalAlignment="Top" Width="74" Click="Disapprove_Click" Foreground="#FF0B637E"/>
    </Grid>
</Window>
