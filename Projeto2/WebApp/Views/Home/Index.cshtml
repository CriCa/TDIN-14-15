@{
    ViewBag.Title = "Book Editor - Web App";
}

<div id="notification-wrapper">
</div>
<div class="container">
    <div class="row">
        <div class="col-md-offset-3 col-md-6">
            <div id="content-wrapper">
                @if (Session["email"] == null)
                {
                    @Html.Partial("_Login")
                }
                else
                {
                    @Html.Partial("_Track")
                }
            </div>
        </div>
    </div>
</div>